{
    "package": "native_rust",
    "version": "1.88.0",
    "type": "native",
    "download": {
        "kind": "tar.gz",
        "sha256": "7c53f4509eda184e174efa6ba7d5eeb586585686ce8edefc781a2b11a7cf512a",
        "url": "https://static.rust-lang.org/dist/rustc-1.88.0-src.tar.gz"
    },
    "dependencies": [
        "all:native_cmake",
        "all:native_openssl",
        "all:native_pkgconf",
        "all:native_python3",
        "all:native_rust_1_87_0"
    ],
    "build": {
        "env": [],
        "steps": [
            "all:echo '[build]' > config.toml",
            "aarch64-apple-darwin::echo 'targets = [\"aarch64-apple-darwin\"]' >> config.toml",
            "x86_64-apple-darwin::echo 'targets = [\"x86_64-apple-darwin\"]' >> config.toml",
            "aarch64-apple-ios::echo 'targets = [\"aarch64-apple-ios\"]' >> config.toml",
            "aarch64-apple-ios-simulator::echo 'targets = [\"aarch64-apple-ios-sim\"]' >> config.toml",
            "x86_64-linux-gnu::echo 'targets = [\"x86_64-unknown-linux-gnu\"]' >> config.toml",
            "aarch64-linux-gnu::echo 'targets = [\"aarch64-unknown-linux-gnu\"]' >> config.toml",
            "aarch64-linux-android::echo 'targets = [\"aarch64-linux-android\"]' >> config.toml",
            "x86_64-linux-android::echo 'targets = [\"x86_64-linux-android\"]' >> config.toml",
            "armv7a-linux-androideabi::echo 'targets = [\"armv7-linux-androideabi\"]' >> config.toml",
            "all:echo 'full-bootstrap = true' >> config.toml",
            "all:echo 'vendor = true' >> config.toml",
            "all:echo 'extended = true' >> config.toml",
            "all:echo 'tools = [\"cargo\"]' >> config.toml",
            "all:echo rustc = \\\"$PREFIX/native/bin/rustc\\\" >> config.toml",
            "all:echo cargo = \\\"$PREFIX/native/bin/cargo\\\" >> config.toml",
            "all:echo '[llvm]' >> config.toml",
            "all:echo 'ninja = false' >> config.toml",
            "all:echo 'download-ci-llvm = false' >> config.toml",
            "all:python3 ./x.py build --stage 3 --verbose -j $NUM_CORES",
            "all:mkdir -p $STAGING_DIR$PREFIX/native/bin",
            "all:cp -a build/*/stage1/lib $STAGING_DIR$PREFIX/native/",
            "all:cp -a build/*/stage1/bin/rustc $STAGING_DIR$PREFIX/native/bin",
            "all:cp -a build/*/stage1-tools-bin/cargo $STAGING_DIR$PREFIX/native/bin/cargo",
            "all:exit 44"
        ]
    }
}