{
    "package": "native_autoconf",
    "version": "2.72",
    "type": "native",
    "download": {
        "kind": "tar.gz",
        "url": "https://ftp.gnu.org/gnu/autoconf/autoconf-2.72.tar.gz",
        "sha256": "afb181a76e1ee72832f6581c0eddf8df032b83e2e0239ef79ebedc4467d92d6e"
    },
    "dependencies": [
        "all:native_make",
        "all:native_m4",
        "all:native_perl"
    ],
    "build": {
        "env": [],
        "steps": [
            "all:./configure --host=$HOST --prefix=$PREFIX/native",
            "all:make -j$NUM_CORES",
            "all:make DESTDIR=$STAGING_DIR install"
        ]
    }
}