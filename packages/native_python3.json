{
    "package": "native_python3",
    "version": "3.11.13",
    "type": "native",
    "download": {
        "kind": "tar.gz",
        "sha256": "0f1a22f4dfd34595a29cf69ee7ea73b9eff8b1cc89d7ab29b3ab0ec04179dad8",
        "url": "https://www.python.org/ftp/python/3.11.13/Python-3.11.13.tgz"
    },
    "build": {
        "env": [],
        "steps": [
            "all:./configure --disable-test-modules --prefix=$PWD/pybuild",
            "all:make -j$NUM_CORES",
            "all:make install",
            "all:mkdir -p $STAGING_DIR$PREFIX/native/python3.11.13",
            "all:cp -r pybuild/* $STAGING_DIR$PREFIX/native/python3.11.13"
        ]
    }
}